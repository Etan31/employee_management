# App environment
NODE_ENV=development
PORT=5000


# PostgreSQL configuration // #change this below according to you pgAdmin
PGHOST=localhost
PGUSER=postgres
PGPASSWORD=your_postgres_password
PGDATABASE=database_name
PGPORT=database_port


DATABASE_URL=postgresql://postgres:your_postgres_password@localhost:database_port/database_name
DOTENV_CONFIG_SILENT=true

JWT_SECRET=secretpass #change this
JWT_EXPIRES_IN=1d
